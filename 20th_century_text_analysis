    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "clean text length: 99779\n",
      "key events of the th century wikipedia move to sidebar hide navigation main pagecontentscurrent eventsrandom articleabout wikipediacontact us contribute helplearn to editcommunity portalrecent changes\n"
     ]
    }
   ],
   "source": [
    "import re\n",
    "text = data.lower()\n",
    "junk_patterns = [\n",
    "    r\"jump to content\",\n",
    "    r\"main menu\",\n",
    "    r\"toggle the table of contents\",\n",
    "    r\"from wikipedia, the free encyclopedia\",\n",
    "]\n",
    "for pat in junk_patterns:\n",
    "    text = re.sub(pat, \" \", text)\n",
    "text = re.sub(r\"[^a-z\\s]\", \" \", text)\n",
    "text = re.sub(r\"\\s+\", \" \", text).strip()\n",
    "\n",
    "print(\"clean text length:\", len(text))\n",
    "print(text[:200])"
   ]
