   "source": [
    "extra_countries = {\n",
    "    \"United States\": [\"us\", \"u.s\", \"u.s.\", \"USA\", \"U.S.A\", \"united states\", \"america\"],\n",
    "    \"United Kingdom\": [\"uk\", \"u.k\", \"britain\", \"england\", \"united kingdom\"],\n",
    "    \"Russia\": [\"russia\", \"russian federation\", \"soviet union\", \"ussr\"],\n",
    "    \"South Korea\": [\"south korea\", \"republic of korea\"],\n",
    "    \"North Korea\": [\"north korea\", \"dprk\"],\n",
    " }\n",
    "extra_counts = []\n",
    "for country, variants in extra_countries.items():\n",
    "        total = 0\n",
    "    \n",
    "        for v in variants:\n",
    "            pattern = r\"\\b\" + re.escape(v.lower()) + r\"\\b\"\n",
    "            total += len(re.findall(pattern, cleaned.lower()))\n",
    "            if total > 0:\n",
    "                extra_counts.append((country, total))\n",
    "                \n",
    "extra_df = pd.DataFrame(extra_counts, columns=[\"Country\", \"Mentions\"])\n",
    "extra_df.sort_values(\"Mentions\", ascending=False)"
   ]
  },
