   "metadata": {},
   "outputs": [],
   "source": [
    "import itertools \n",
    "relationships = []\n",
    "for sent in doc.sents:\n",
    "    countries_in_sentence = [\n",
    "        ent.text for ent in sent.ents\n",
    "        if ent.label_ == \"GPE\"]\n",
    "    countries_in_sentence = list(set(countries_in_sentence))\n",
    "    if len(countries_in_sentence) > 1:\n",
    "        pairs = list(itertools.combinations(countries_in_sentence, 2))\n",
    "        relationships.extend(pairs)"
   ]
  },
