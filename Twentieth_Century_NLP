    "replace_dict = {\n",
    "    \"the United States\": \"United States\",\n",
    "    \"The United States\": \"United States\",\n",
    "    \"U.S.\": \"United States\",\n",
    "    \"United States\": \"United States\",\n",
    "    \"Unites States\": \"United States\",\n",
    "    \"New York\": \"United States\",\n",
    "    \"U.S\": \"United States\",\n",
    "    \"US\": \"United States\",\n",
    "    \"Britain\": \"United Kingdom\",\n",
    "    \"USSR\": \"Russia\",\n",
    "    \"Moscow\": \"Russia\",\n",
    "    \"the Soviet Union\": \"Russia\",\n",
    "    \"Soviet Union\": \"Russia\",\n",
    "    \"Berlin\": \"Germany\"}\n",
    "\n",
    "country_counts_ner[\"Country\"] = country_counts_ner[\"Country\"].replace(replace_dict)\n",
    "\n",
    "country_counts_ner = (\n",
    "    country_counts_ner\n",
    "    .groupby(\"Country\", as_index=False)[\"Mentions\"]\n",
    "    .sum()\n",
    "    .sort_values(by=\"Mentions\", ascending=False)\n",
    ")\n",
    "country_counts_ner.head(10)"
   ]
