   "source": [
    "country_counts = []\n",
    "\n",
    "for c in countries:\n",
    "    pattern = r\"\\b\" + re.escape(c.lower()) + r\"\\b\" \n",
    "    count = len(re.findall(pattern, cleaned.lower()))\n",
    "\n",
    "    if count > 0:\n",
    "        country_counts.append((c, count))\n",
    "\n",
    "country_df = pd.DataFrame(country_counts, columns=[\"Country\", \"Mentions\"])\n",
    "\n",
    "country_df = country_df.sort_values(by=\"Mentions\", ascending=False).reset_index(drop=True)\n",
    "\n",
    "country_df.head(20)"
   ]
  },
